{
  "name": "hideberg-server",
  "version": "0.0.0",
  "private": true,
  "author": "Elmahdi KORFED, Fabrice JAUVAT",
  "main": "./bin/www",
  "scripts": {
    "start": "nodemon bin/www --exec babel-node --presets es2015",
    "debug": "babel-node --inspect ./bin/www --exec babel-node --presets es2015",
    "test": "mocha --require babel-polyfill --compilers js:babel-register",
    "apidoc": "apidoc -i routes/ -o ./public/apidoc/",
    "start-pm2": "pm2 start --interpreter babel-node bin/www --watch --ignore-watch='node_modules' --log-date-format='YYYY-MM-DD HH:mm Z' -f",
    "start-cluster-pm2": "pm2 start --interpreter babel-node bin/www -i max",
    "log-pm2": "pm2 logs --log-date-format='YYYY-MM-DD HH:mm Z' --lines 1000",
    "kill-pm2": "pm2 kill"
  },
  "dependencies": {
    "bcrypt": "^1.0.3",
    "body-parser": "~1.16.0",
    "compression": "^1.7.1",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "debug": "~2.6.0",
    "errorhandler": "^1.5.0",
    "express": "~4.14.1",
    "express-jwt": "^5.3.0",
    "helmet": "^3.9.0",
    "https": "^1.0.0",
    "jade": "~1.11.0",
    "jsonwebtoken": "^8.1.0",
    "line-reader": "^0.4.0",
    "morgan": "~1.7.0",
    "multer": "^1.3.0",
    "mysql": "^2.15.0",
    "mysql2": "^1.4.2",
    "pug": "~2.0.0-beta11",
    "sequelize": "^4.31.2",
    "serve-favicon": "~2.3.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1"
  }
}
